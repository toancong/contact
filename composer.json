{
    "name": "beanbean/contact",
    "version": "0.1.0",
    "description": "A package for Laravel for save multiple contact",
    "license": "MIT",
    "authors": [
        {
            "name": "Toan Cong",
            "email": "toancong1920@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "cloudcreativity/laravel-json-api": "^1.6"
    },
    "require-dev": {
        "mockery/mockery": "~1.0",
        "orchestra/testbench": "^3.7",
        "phpunit/phpunit": "~7.0"
    },
    "autoload": {
        "psr-4": {
            "Bean\\Contact\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "database/factories"
        ],
        "psr-4": {
            "Bean\\Contact\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bean\\Contact\\ServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
